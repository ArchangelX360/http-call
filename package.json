{
  "name": "http-call",
  "description": "make http requests",
  "version": "3.0.2",
  "author": "Jeff Dickey @jdxcode",
  "bugs": "https://github.com/jdxcode/http-call/issues",
  "dependencies": {
    "debug": "^3.0.1",
    "is-retry-allowed": "^1.1.0",
    "is-stream": "^1.1.0",
    "tunnel-agent": "^0.6.0"
  },
  "devDependencies": {
    "@types/is-stream": "^1.1.0",
    "@types/jest": "^20.0.8",
    "@types/nock": "^8.2.1",
    "@types/node": "^8.0.28",
    "jest": "^21.0.1",
    "nock": "^9.0.14",
    "rimraf": "^2.6.1",
    "standard": "^10.0.3",
    "ts-jest": "^21.0.0",
    "typescript": "^2.5.2"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/jdxcode/http-call",
  "jest": {
    "coverageReporters": [
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  },
  "keywords": [
    "http",
    "request",
    "rest"
  ],
  "license": "ISC",
  "main": "lib/http.js",
  "repository": "jdxcode/http-call",
  "scripts": {
    "coverage": "cat coverage/coverage-final.json | remap-istanbul -o coverage/coverage-final.json && curl -s https://codecov.io/bash | bash",
    "precommit": "lint-staged",
    "prepare": "rimraf lib && tsc && rimraf 'lib/**/*.test.js' 'lib/**/*.test.d.ts'",
    "pretest": "tsc --sourcemap",
    "test": "jest"
  }
}
